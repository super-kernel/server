{
    "name": "super-kernel/server",
    "description": "The server component for super-kernel.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "super-kernel",
        "server"
    ],
    "autoload": {
        "psr-4": {
            "SuperKernel\\Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuperKernelTest\\Server\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "wheakerd",
            "email": "wheakerd@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4",
        "ext-swoole": ">=6.0",
        "super-kernel/command": "*",
        "super-kernel/config": "*"
    },
    "require-dev": {
        "swoole/ide-helper": "^6.0",
        "super-kernel/di": "@dev",
        "super-kernel/event-dispatcher": "@dev",
        "super-kernel/framework": "@dev",
        "super-kernel/logger": "@dev"
    }
}
